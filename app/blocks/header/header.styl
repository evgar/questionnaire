.file-header
	display flex
	flex-flow column nowrap
	margin 0 0 69px 0
	+below(2)
		margin 0 0 10px 0
	.header-logos
		display flex
		flex-flow row wrap
		justify-content space-between
		&__csssr
			margin 60px 0 0 11%
			+below(2)
				margin 60px 0 0 30%
			+below(1)
				display none
		&__filled-by-me
			margin 47px 5.5% 0 0
			+below(2)
				margin 5px 0 4px 21%
			+below(1)
				display none
	.header-title
		display flex
		justify-content center
		margin 7px 4% 0% 0%
